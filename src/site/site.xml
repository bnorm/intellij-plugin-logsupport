<project xmlns="http://maven.apache.org/DECORATION/1.0.0"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0
		 	http://maven.apache.org/xsd/decoration-1.0.0.xsd">

	<publishDate position="right"/>

	<body>
		<head>
			<meta name="keywords" content="log, framework, intellij, idea, plugin, 
				log4j, logback, slf4j, java.util.logging, logger, logfactory,
				commons-logging"/>
			<script type="text/javascript" src="http://piwik.tinyjee.org/piwik.js"></script>
			<script type="text/javascript">
				try {
				var piwikTracker = Piwik.getTracker("http://piwik.tinyjee.org/piwik.php", 5);
				piwikTracker.trackPageView();
				piwikTracker.enableLinkTracking();
				} catch( err ) {}
			</script>
		</head>

		<breadcrumbs>
			<item name="Log Support" href="index.html"/>
		</breadcrumbs>

		<menu name="Overview">
			<item name="About" href="index.html"/>
			<item name="Download" href="http://plugins.intellij.net/plugin/?idea&amp;id=4953"/>
			<item name="Release Notes" href="http://plugins.intellij.net/plugin/?idea&amp;id=4953"/>
		</menu>

		<menu name="Guides">
			<item name="Quick Start Guide" href="quickStartGuide.html"/>
			<item name="Log ID Guide" href="logIDGuide.html"/>
			<item name="Log Review Guide" href="logReviewGuide.html"/>
		</menu>

		<menu name="Resources">
			<item name="SF Project Page" href="http://sourceforge.net/projects/logsupport/"/>
			<item name="Built with Hudson" href="http://www.tinyjee.org/hudson/job/Log%20Support%20-%20IDEA%20Plugin%20(Trunk)/"/>
			<item name="QA'ed by Sonar" href="http://www.tinyjee.org/sonar/"/>
		</menu>

		<menu ref="reports"/>
	</body>

	<bannerLeft>
		<alt>${project.name}</alt>
		<src>/images/logo.jpg</src>
	</bannerLeft>

	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-stylus-skin</artifactId>
		<version>1.0.1</version>
	</skin>
</project>